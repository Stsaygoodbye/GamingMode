            echo "250" > /proc/sys/vm/dirty_expire_centisecs
            echo "800" > /proc/sys/vm/dirty_writeback_centisecs
            echo "5" > /proc/sys/vm/dirty_background_ratio
            echo "10" > /proc/sys/vm/dirty_ratio
